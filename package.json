{
  "name": "bruno-api-catalog",
  "version": "1.0.0",
  "description": "Automated catalog of OpenAPI specifications from GitHub with Bruno integration",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "node -r dotenv/config index.js dotenv_config_path=.env.local",
    "preinstall": "bash railway-build.sh",
    "postinstall": "cd bruno-doc-gen && npm install --omit=dev"
  },
  "keywords": [
    "bruno",
    "openapi",
    "api",
    "catalog",
    "documentation",
    "github"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "archiver": "^7.0.1",
    "axios": "^1.13.1",
    "better-sqlite3": "^9.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "uuid": "^8.3.2"
  }
}
